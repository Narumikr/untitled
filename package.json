{
  "name": "@naru/untitled-ui-library",
  "version": "1.1.0",
  "description": "TypeScript components library for React and utility functions",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "style": "dist/color/sekai-colors.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./color/sekai-colors.css": {
      "import": "./dist/color/sekai-colors.css",
      "require": "./dist/color/sekai-colors.css"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "generate-index": "python scripts/generator.py",
    "build": "rm -rf dist && npm run generate-index && rollup -c && tsc --noEmit && sh scripts/build_local.sh",
    "lint": "npx eslint .  --no-cache --fix --max-warnings=0",
    "prettier": "npx prettier --write . --ignore-path .prettierignore",
    "plop": "npx plop component",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.28.3",
    "@chromatic-com/storybook": "3.2.7",
    "@eslint/compat": "1.3.2",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.39.1",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-image": "3.0.3",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-onboarding": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@svgr/rollup": "8.1.0",
    "@types/node": "22.17.2",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.34.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "3.10.1",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-storybook": "10.0.6",
    "eslint-plugin-unused-imports": "4.2.0",
    "plop": "4.0.1",
    "postcss": "8.5.6",
    "rollup": "4.48.0",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-dts": "6.2.3",
    "rollup-plugin-postcss": "4.0.2",
    "rollup-plugin-tsconfig-paths": "1.5.2",
    "rollup-plugin-typescript2": "0.36.0",
    "sass": "1.90.0",
    "storybook": "8.6.14",
    "typescript": "5.9.2",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "6.3.5"
  },
  "peerDependencies": {
    "@types/react": "^18.2.63",
    "@types/react-dom": "^18.2.18",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
